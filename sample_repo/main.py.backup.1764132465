#!/usr/bin/env python3
"""
Main application file with intentional issues for self-healing agent testing.
"""

import os
import sys
import time
import logging

def process_data(data_list):
    """Process a list of data items with debugging prints."""
    logging.info(f"Starting to process {len(data_list)} items")
    
    results = []
    for i, item in enumerate(data_list):
        logging.info(f"Processing item {i}: {item}")
        
        # Simulate some processing - removed blocking time.sleep for performance improvement
        # time.sleep(0.1) 
        processed_item = str(item).upper()
        results.append(processed_item)
        
        # More debug output
        logging.info(f"Processed: {processed_item}")
    
    logging.info("Processing complete!") # Replaced print with logging.info
    return results

def main():
    """Main function with performance issues."""
    # Configure logging at the start of the application
    logging.basicConfig(level=logging.INFO, format='%(asctime)s - %(levelname)s - %(message)s')
    
    logging.info("Application starting...") # Replaced print with logging.info
    
    # Sample data
    sample_data = ["hello", "world", "python", "healing", "agent"] # Fixed: Missing closing bracket added
    
    # Process data (this will be slow due to sleep, now improved)
    results = process_data(sample_data)
    
    # Display results with more logs
    logging.info("Final results:") # Replaced print with logging.info
    for result in results:
        logging.info(f"- {result}") # Replaced print with logging.info
    
    logging.info("Application finished successfully!") # Replaced print with logging.info
    return results

if __name__ == "__main__":
    main()